<!DOCTYPE html>
<html>
<head>
  <title>Trashtrack</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'default', 'data-turbolinks-track' => true %>
  <!--<%= csrf_meta_tags %>-->
  <script src="//maps.google.com/maps/api/js?v=3.18&sensor=false&client=&key=&libraries=geometry&language=&hl=&region="></script> 
<script src="//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/infobox/1.1.9/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->

</head>
<body>
<div class="container">
<nav>
	<div>
	<% if user_signed_in? %>
	<div style="float:left">
		<p><%= link_to "Profile Settings", edit_user_registration_path %></p>
		<p> <%= button_to "Sign Out", destroy_user_session_path, method: :delete %></p>
	</div>  
		
	<% else %>
	<div style="float:left"><%= link_to "Sign up now", new_user_registration_path %> | <%= link_to "Log In", new_user_session_path %> | <%= link_to "Post", posts_path	%> 
	</div>
		
	<% end %>

	</div>
	<br/>
	<br/>
		<div>
			<% if notice || alert %>
			<%= "#{notice || ""}#{alert || ""}" %>
			<% end %>
		</div>
	

</nav>
<br/>




<%= yield %>

</div>

</body>
</html>
